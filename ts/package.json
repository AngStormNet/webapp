{
  "name": "webapp-typescript",
  "version": "1.4.2",
  "description": "React Boilerplate with support for Redux, TypeScript, Hot Reloading, Routing, Isomorphic rendering, TSLint, SASS, Testing",
  "main": "index.tsx",
  "repository": "https://github.com/erik-sn/webapp",
  "scripts": {
    "new": "ts-node ./config/new_component.ts",
    "dev": "ts-node ./config/server.ts",
    "test": "mocha -R min --compilers ts:ts-node/register,tsx:ts-node/register --require ./__tests__/helper.tsx --recursive ./__tests__",
    "tdd": "npm run test -- --watch",
    "cover": "istanbul cover -x **/__tests__/** -e .tsx node_modules/mocha/bin/_mocha -- --compilers ts:ts-node/register,tsx:ts-node/register --require ./__tests__/helper.tsx --recursive ./__tests__",
    "lint": "tslint -c tslint.json 'src/**/*.ts*' --fix",
    "compile": "tsc --outdir dist --sourcemap false",
    "build": "webpack --config ./config/webpack.production.config.ts --progress --profile --colors",
    "production": "npm run lint && npm run test && npm run compile && npm run build",
    "start": "node ./dist/config/server.production.js"
  },
  "author": "Erik Niehaus",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/compression": "^0.0.34",
    "@types/enzyme": "^2.8.0",
    "@types/express": "^4.0.35",
    "@types/jsdom": "^11.0.1",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.24",
    "@types/react": "15.5.0",
    "@types/react-dom": "^15.5.4",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-redux": "^5.0.4",
    "@types/react-router": "^4.0.3",
    "@types/react-router-dom": "^4.0.2",
    "@types/redux": "^3.6.0",
    "@types/redux-promise": "^0.5.24",
    "@types/redux-thunk": "^2.1.0",
    "@types/sinon": "^2.1.2",
    "@types/webpack": "^2.2.15",
    "@types/webpack-dev-server": "^1.12.6",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "7.1.2",
    "awesome-typescript-loader": "^3.1.2",
    "chai": "^4.1.1",
    "codecov": "^2.1.0",
    "css-loader": "^0.28.0",
    "dirty-chai": "^2.0.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "^11.2.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "v3.0.0-beta.6",
    "react-test-renderer": "15.5.1",
    "sass-loader": "^6.0.3",
    "sinon": "^3.2.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "tslint-react": "^3.2.0",
    "typescript-require": "^0.2.9-1",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "history": "^4.6.1",
    "morgan": "^1.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.4",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  }
}
