    "@types/extract-text-webpack-plugin": "^2.0.0",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.38",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.0",
    "@types/react": "^15.0.1",
    "@types/react-dom": "^0.14.20",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.45",
