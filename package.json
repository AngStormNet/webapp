{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate with support for Redux, TypeScript, Hot Reloading, Routing, Isomorphic rendering, ESLint and SASS",
  "main": "index.js",
  "repository": "",
  "scripts": {
    "dev": "node server.js",
    "test:watch": "npm run test -- --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "build": "webpack -p --config webpack.production.config.js --progress --profile --colors",
    "start": "nodemon server.production.js --exec babel-node"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "es6-promise": "~3.2.1",
    "eslint": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.4.5",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-react": "^1.0.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "webpack": "^1.12.9"
  }
