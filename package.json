{
  "name": "tsreact",
  "version": "1.2.0",
  "description": "React Boilerplate with support for Redux, TypeScript, Hot Reloading, Routing, Isomorphic rendering, TSLint and SASS",
  "main": "index.tsx",
  "repository": "https://github.com/kiresuah/ReactTypeScript",
  "scripts": {
    "dev": "ts-node ./config/server.ts",
    "test:watch": "npm run test -- --watch",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --require ./test/test_helper.tsx --recursive ./test",
    "build": "webpack -p --config ./config/webpack.production.config.ts --progress --profile --colors",
    "start": "nodemon ./config/server.production.tsx --exec ts-node",
    "lint": "tslint ./src ./test --fix"
  },
